<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Main">

    <TextView

        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:id="@+id/encabezado"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"

        android:fontFamily="@font/oceanoestilo"
        android:textSize="30dp"
        android:paddingTop="25dp"
        android:text="sfdg"

        android:background="@color/md_theme_onTertiaryContainer"
        />
    <TextView

        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:id="@+id/encabezado2"
        app:layout_constraintTop_toBottomOf="@+id/encabezado"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:textStyle="italic"

        android:textSize="30dp"
        android:paddingTop="25dp"
        android:text="sfdg"

        android:background="@color/md_theme_tertiaryFixedDim"
        />


    <androidx.viewpager.widget.ViewPager
        android:id="@+id/aplicacionViewPager"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/encabezado2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        />






</androidx.constraintlayout.widget.ConstraintLayout>